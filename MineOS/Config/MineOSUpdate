--#########################################################
response = http.get("https://raw.githubusercontent.com/Pugduddly/MineOS/master/MineOS/Config/OSversion")
if response then
	OSver = response.readAll()
	if OSver == ONver then
		write("MineOS is up to date!")
	else
		write("Please update MineOS!")
	end
else
	write("No connection D:")
end
term.setCursorPos(31,2)
write("Version         ")
version = fs.open("MineOS/Config/OSversion", "r")
write(version.readLine())
version.close()
