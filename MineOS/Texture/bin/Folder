function moveimage1()
resetScreen()
scp(10,2)
w(" Task Manager     ")
sbc(16384)
w("X")
sbc(256)
scp(10,3)
w("                   ")
scp(10,4)
w(" -MineOS.exe   [X] ")
scp(10,5)
w(" -                 ")
scp(12,5)
scp(10,6)
w(" -                 ")
scp(10,7)
w(" -                 ")
scp(10,8)
w("                   ")
end

