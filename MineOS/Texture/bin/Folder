stc(16384)
scp(44,13)
write(". ")
scp(48,13)
write(". ")

sleep(1)

sbc(1)
stc(16384)
scp(44,13)
write(" .")
scp(48,13)
write(" .")

sleep(1)

sbc(356)
stc(1)
