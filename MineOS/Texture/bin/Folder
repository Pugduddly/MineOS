stc(1)
sbc(8192)
scp(1,19)
w([[ Start ]])
sbc(2048)
w([[                                       00:00]])
stc(16384)
scp(13,19)
end
sbc(1)
stc(16384)
scp(44,13)
write(" .")
scp(48,13)
write(". ")

sleep(2)

