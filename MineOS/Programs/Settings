	scp(17,7)
	sbc(258)
	stc(128)
	w("")
	scp(17,7)
	stc(32768)
	fileaddB = read()
	  if fileaddB == "" then
r("MineOS/Programs/Settings")
error("Terminated")
	  elseif fs.exists(fileaddB) then
	  sbc(1)
	  scp(1,17)
	    fs.delete("MineOS/Texture/Desktop/"..dataname1a.."/1")
	    fs.copy(fileaddB, "MineOS/Texture/Desktop/"..dataname1a.."/1")
		sbc(256)
		scp(17,7)
		stc(16384)
		w("File added!    ")
        sleep(1)
