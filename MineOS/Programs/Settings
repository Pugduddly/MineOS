	fileaddB2 = read()
	  if fileaddB2 == "" then
        r("MineOS/Programs/Settings")
        error("Terminated")
      else

	local response = http.get(
		"http://pastebin.com/raw.php?i="..textutils.urlEncode( fileaddB2 )
		)
		
	if response then
fs.delete("MineOS/Texture/Desktop/"..dataname1a.."/1")
		local sResponse = response.readAll()
		response.close()
		
		local file = fs.open("MineOS/Texture/Desktop/"..dataname1a.."/1", "w" )
		file.write( sResponse )
