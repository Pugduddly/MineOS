error("Terminated")
	  elseif not fs.exists(fileaddB) then
	    scp(17,7)
		stc(16384)
		w("File not found!")
        sleep(2)
r("MineOS/Programs/Settings")
error("Terminated")
	  end
    break
   end
--############## Custom # Pastebin ###############--
if X >= 26 and X <= 35 and Y == 9 then 
