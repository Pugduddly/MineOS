os.loadAPI("/MineOS/API/BSOD")
os.loadAPI("/MineOS/API/DialogBox")
local function main()
    local input = DialogBox.inputBox("Run")
    if input then
        local nTask = shell.openTab(input)
        if nTask then
            shell.switchTab(nTask)
        else
            DialogBox.dialogBox("Error", "Can't run program!", {"OK"})
        end
    end
end
BSOD.doErrorTrap(main)
